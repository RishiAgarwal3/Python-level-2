print ("1. farenhiet")
print ("2.celcius")
print ("")
choice = int(input("press 1 or 2 according to which value u want to conver the temperature into \n"))

if choice == 1:
    celcius = int(input("Enter the temperature in celcius: \n"))

    farenhite = (celcius*9/5+32)

    print ("the temperature in farenhite is: ", farenhite)
elif choice == 2:
    f1 = int(input("Enter the temperature in farenheight: \n"))

    celcius1 = 5/9(f1-32)

    print ("the temperature in celcius is: ", celcius1)
else :
    print ("enter from the above options only")
