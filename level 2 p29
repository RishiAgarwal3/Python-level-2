nums = [1,1,2,4,5,67,33,4,4,3,2,5,7,8,7,7,8]
list1 = []

while  len(nums)>0:
    for i in nums:
        if i not in list1:
            list1.append(i)
            nums.remove(i)
        else:
            nums.remove(i)

